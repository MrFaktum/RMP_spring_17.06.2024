<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#000000" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>
    <item android:top="4dp"
        android:bottom="4dp"
        android:right="4dp"
        android:left="4dp">
        <shape android:shape="oval">
            <solid android:color="#D6D6D6" />
            <size
                android:width="38dp"
                android:height="38dp" />
        </shape>
    </item>
    <item
        android:width="6dp"
        android:height="6dp"
        android:top="7dp"
        android:gravity="center_horizontal"
        android:drawable="@drawable/pause_color">
    </item>
    <item
        android:width="6dp"
        android:height="18dp"
        android:top="10dp"
        android:drawable="@drawable/pause_color"
        android:gravity="center">
    </item>
</layer-list>